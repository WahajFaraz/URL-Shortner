{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    },
    {
      "source": "/favicon.svg",
      "destination": "/favicon.svg"
    },
    {
      "source": "/",
      "destination": "/api/status"
    },
    {
      "source": "/favicon.ico",
      "destination": "/favicon.svg"
    },
    {
      "source": "/favicon.png",
      "destination": "/favicon.svg"
    },
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ]
}

